<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tih.miwork">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="MiWork"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Numbers"
            android:banner="@color/category_numbers"
            android:label="Numbers"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".family"
            android:label="Family"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".Colors"
            android:label="Colors"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".Phrases"
            android:label="Phrases"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".list_item"></activity>
    </application>

</manifest>